{
  "name": "nephren-ui-revamp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "local": "vite --port 7000",
    "dev": "vite",
    "dev:watch": "concurrently \"npm run build:watch\" \"npm run preview\"",
    "build": "tsc -b && vite build",
    "build:watch": "tsc -b && vite build --watch",
